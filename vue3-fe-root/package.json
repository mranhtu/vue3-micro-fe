{
  "name": "@micro/vue3-fe-root",
  "version": "0.0.1",
  "scripts": {
    "start": "webpack serve --mode=development --port 8000 --env isLocal --open",
    "start:pre": "cross-env APP_ENV=pre webpack serve --mode=development --port 8000 --env isLocal",
    "start:stg": "cross-env APP_ENV=stg webpack serve --mode=development --port 8000 --env isLocal",
    "lint": "eslint src",
    "test": "jest --passWithNoTests",
    "format": "prettier --write './**'",
    "build": "webpack --mode=production",
    "build:pre": "cross-env APP_ENV=pre webpack --mode=production",
    "build:dev": "webpack --mode=production"
  },
  "files": [
    "dist",
    "cicd"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && eslint src"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@types/systemjs": "^6.1.1",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.2.0",
    "eslint-config-important-stuff": "^1.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-cli": "^27.3.1",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "webpack": "^5.62.2",
    "webpack-cli": "^4.9.1",
    "webpack-config-single-spa": "^5.1.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "import-map-overrides": "^3.0.0",
    "jquery": "^3.6.0",
    "single-spa": "^5.9.3",
    "single-spa-layout": "1.6.0",
    "systemjs": "^6.14.1",
    "vue": "^3.3.4",
    "vue-router": "4.0.3"
  }
}
